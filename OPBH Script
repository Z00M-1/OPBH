local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Z00M-Hub",
   Icon = 0,
   LoadingTitle = "OPBH Script",
   LoadingSubtitle = "by Z00M",
   ShowText = "Script",
   Theme = "Default",
   ToggleUIKeybind = "K",
   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "OPBH",
      FileName = "Z00M Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

local Tab = Window:CreateTab("Main", 4483362458)

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")

-- Function to get all tool names in the player's backpack
local function getToolNames()
    local tools = {}
    for _, item in pairs(backpack:GetChildren()) do
        if item:IsA("Tool") then
            table.insert(tools, item.Name)
        end
    end
    return tools
end

-- Create the dropdown with dynamic tool names
local Dropdown = Tab:CreateDropdown({
    Name = "Select a Tool",
    Options = getToolNames(), -- dynamically filled list of tools
    CurrentOption = {}, -- Can be left empty unless you want to preselect
    MultipleOptions = false,
    Flag = "ToolDropdown",
    Callback = function(Options)
        print("Selected Tool:", Options[1])
        -- You can also handle equipping the tool or any other logic here
    end,
})

-- Optional: Refresh the dropdown if inventory changes
backpack.ChildAdded:Connect(function(child)
    if child:IsA("Tool") then
        Dropdown:Refresh(getToolNames(), true) -- true to keep current selection
    end
end)

backpack.ChildRemoved:Connect(function(child)
    if child:IsA("Tool") then
        Dropdown:Refresh(getToolNames(), true)
    end
end)


local WalkSpeedSlider = Tab:CreateSlider({
    Name = "WalkSpeed",
    Range = {16, 300},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 16,
    Flag = "WalkSpeedSlider",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end,
})

 JumpHeightSlider = Tab:CreateSlider({
   Name = "Jump Height",
   Range = {50, 250},
   Increment = 10,
   Suffix = "Jump Power",
   CurrentValue = 50,
   Flag = "JumpHeightSlider",
   Callback = function(Value)
       local player = game.Players.LocalPlayer
       if player and player.Character then
           local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
           if humanoid then
               humanoid.JumpPower = Value
           end
       end
   end,
})

local TabQuests = Window:CreateTab("Quests", 4483362458)

local function teleportTo(position)
    game.Players.LocalPlayer.Character:MoveTo(position)
end

local function createQuestButton(name, position)
    return TabQuest:CreateButton({
        Name = name,
        Callback = function()
            teleportTo(position)
        end
    })
end


