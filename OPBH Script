local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Z00M-Hub",
   Icon = 0,
   LoadingTitle = "OPBH Script",
   LoadingSubtitle = "by Z00M",
   ShowText = "Script",
   Theme = "Default",
   ToggleUIKeybind = "K",
   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false,
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "OPBH",
      FileName = "Z00M Hub"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

local Tab = Window:CreateTab("Main", 4483362458)

local WalkSpeedSlider = Tab:CreateSlider({
    Name = "WalkSpeed",
    Range = {16, 300},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 16,
    Flag = "WalkSpeedSlider",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end,
})

 JumpHeightSlider = Tab:CreateSlider({
   Name = "Jump Height",
   Range = {50, 250},
   Increment = 10,
   Suffix = "Jump Power",
   CurrentValue = 50,
   Flag = "JumpHeightSlider",
   Callback = function(Value)
       local player = game.Players.LocalPlayer
       if player and player.Character then
           local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
           if humanoid then
               humanoid.JumpPower = Value
           end
       end
   end,
})

local TabQuests = Window:CreateTab("Quests", 4483362458)

local function createQuestButton(name, callback)
    return TabQuests:CreateButton({
        Name = name,
        Callback = callback
    })
end

createQuestButton("Bandit", function() end)
createQuestButton("Snow Bandit", function() end)
createQuestButton("Savages", function() end)
createQuestButton("Toxic Zombie", function() end)
createQuestButton("Skeleton", function() end)
createQuestButton("Sky Warriors", function() end)
createQuestButton("Obsidmon", function() end)
createQuestButton("Suney", function() end)

local TabIslands = Window:CreateTab("Islands", 4483362458)

local function teleportTo(position)
    game.Players.LocalPlayer.Character:MoveTo(position)
end

local function createIslandButton(name, position)
    return TabIslands:CreateButton({
        Name = name,
        Callback = function()
            teleportTo(position)
        end
    })
end

createIslandButton("Teleport To Spawn", Vector3.new(-1446.97, 16.51, 1353.49))
